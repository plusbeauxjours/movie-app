{
  "expo": {
    "name": "Popcorn Movie",
    "githubUrl": "https://github.com/plusbeauxjours/movie-app",
    "description": "Beautiful App to find Movies and TV Shows",
    "slug": "movie-app",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.12",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "./assets/iconCorn.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "movie.plusbeauxjours"
    },
    "android": {
      "versionCode": 12,
      "package": "movie.plusbeauxjours"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  }
}
